<?xml version="1.0" encoding="UTF-8"?>
<cgmm-properties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="cgmm-properties.xsd">
<cgmm-information>
		<cgmm-context-name>@cgmm-context-name@</cgmm-context-name>
		<cgmm-login-config-file-name>@cgmm-login-config-file-name@</cgmm-login-config-file-name>
		<start-auto-syncgts>@start-auto-syncgts@</start-auto-syncgts>
		<cgmm-new-grid-user-creation-disabled>@cgmm-new-grid-user-creation-disabled@</cgmm-new-grid-user-creation-disabled>
		<cgmm-new-grid-user-creation-host-redirect-uri>@cgmm-new-grid-user-creation-host-redirect-uri@</cgmm-new-grid-user-creation-host-redirect-uri>
		<cgmm-alternate-behavior>@cgmm-alternate-behavior@</cgmm-alternate-behavior>
		<cgmm-standalone-mode>@cgmm-standalone-mode@</cgmm-standalone-mode>
	</cgmm-information>
	<host-application-information>
		<host-context-name>@host-context-name@</host-context-name>
		<host-application-name-for-csm>@application.context.name@</host-application-name-for-csm>
		<host-public-home-page-url>@host-public-home-page-url@</host-public-home-page-url>
        <host-user-home-page-url>@host-user-home-page-url@</host-user-home-page-url>
        <host-user-login-page-url>@host-user-login-page-url@</host-user-login-page-url>
        <host-new-local-user-creation-url>@host-new-local-user-creation-url@</host-new-local-user-creation-url>
		<host-mail-jndi-name>@mail.jndi.name@</host-mail-jndi-name>
		<host-mail-email-id-to>@host-mail-email-id-to@</host-mail-email-id-to>
		<host-mail-email-id-from>@host-mail-email-id-from@</host-mail-email-id-from>	
		<host-mail-email-subject>@host-mail-email-subject@</host-mail-email-subject>
		<host-application-logo-url>images/appLogo.gif</host-application-logo-url>
		<host-application-logo-alt-text>@host-application-logo-alt-text@</host-application-logo-alt-text>
	</host-application-information>

	<authentication-service-list>
		<authentication-service-information>
			<service-name>@SERVICE_NAME@</service-name>
			<service-url>@CAGRID_AUTHENTICATION_SERVICE_URL@</service-url>
			<dorian-information>
				<service-url>@CAGRID_DORIAN_SERVICE_URL@</service-url>
				<proxy-lifetime-hours>12</proxy-lifetime-hours>
				<proxy-lifetime-minutes>0</proxy-lifetime-minutes>
				<proxy-lifetime-seconds>0</proxy-lifetime-seconds>
				<proxy-delegation-path-length>3</proxy-delegation-path-length>
			</dorian-information>
		</authentication-service-information>
	</authentication-service-list>
</cgmm-properties>