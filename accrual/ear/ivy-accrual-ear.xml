    <dependency org="gov.nih.nci.coppa.pa"   name="pa-ejb-client"           rev="3.6.1-alpha1"        force="true"   conf="runtime->master"/>
    <dependency org="commons-collections"    name="commons-collections"     rev="3.2.1"             force="true"   conf="runtime->master"/>
