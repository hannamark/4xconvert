    <dependency org="commons-io"             name="commons-io"              rev="1.4"               force="true"   conf="runtime->master"/>
    <dependency org="commons-lang"           name="commons-lang"            rev="2.4"               force="true"   conf="runtime->master"/>
    <dependency org="commons-validator"      name="commons-validator"       rev="1.3.0"                            conf="runtime->master"/>
    <dependency org="net.sf.ehcache"         name="ehcache"                 rev="1.2.4"                            conf="runtime->master">
        <exclude module="commons-logging"/>
    </dependency>
    <dependency org="net.sf.opencsv"         name="opencsv"                 rev="2.1"              conf="runtime->master"/>
  </dependencies>

