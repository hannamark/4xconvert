<ivy-module version="1.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="cbiit" module="ctrp-accrual-ear"/>

  <configurations>
    <conf name="runtime"/>
    <conf name="container"/>
  </configurations>

  <dependencies>
    <!-- runtime -->
    <dependency org="gov.nih.nci.coppa.pa"   name="pa-commons"              rev="${pa.clients.version}"        force="true"   conf="runtime->master"/>
    <dependency org="gov.nih.nci.coppa.pa"   name="pa-ejb-client"           rev="${pa.clients.version}"        force="true"   conf="runtime->master"/>
    <dependency org="commons-collections"    name="commons-collections"     rev="3.2.1"             force="true"   conf="runtime->master"/>
    <dependency org="commons-fileupload"     name="commons-fileupload"      rev="1.2.1"             force="true"   conf="runtime->master"/>
    <dependency org="commons-io"             name="commons-io"              rev="1.4"               force="true"   conf="runtime->master"/>
    <dependency org="commons-lang"           name="commons-lang"            rev="2.4"               force="true"   conf="runtime->master"/>
    <dependency org="commons-validator"      name="commons-validator"       rev="1.3.0"                            conf="runtime->master"/>
    <dependency org="net.sf.ehcache"         name="ehcache"                 rev="1.2.4"                            conf="runtime->master">
        <exclude module="commons-logging"/>
    </dependency>
    <dependency org="joda-time"              name="joda-time"               rev="1.6.2"           conf="runtime->master"/>
    <dependency org="de.jollyday"            name="jollyday"                rev="0.4.5"           conf="runtime->master" transitive="false"/>
    <dependency org="net.sf.opencsv"         name="opencsv"                 rev="2.1"              conf="runtime->master"/>
  </dependencies>

</ivy-module>
