<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
 <constant name="struts.devMode" value="true" />
	<package name="default" extends="struts-default">
		<interceptors>
		    <interceptor name="nci-commonsParams" class="com.fiveamsolutions.nci.commons.web.struts2.interceptor.DisplayTagParametersInterceptor" />
		</interceptors>		
		<action name="logout" class="gov.nih.nci.pa.action.Logout" method="logout">
		 	<result>/index.jsp</result>
		</action>
		<action name="login">
			<result>/WEB-INF/jsp/login.jsp</result>
		</action>	   
	</package>
	<package name="protected" namespace="/protected" extends="default">
		<action name="studyProtocolQuery" class="gov.nih.nci.pa.action.StudyProtocolQueryAction" method="query">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/WEB-INF/jsp/studyProtocolQueryCriteria.jsp</result>
		</action>
		<action name="studyProtocolQueryHome" class="gov.nih.nci.pa.action.StudyProtocolQueryAction">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/WEB-INF/jsp/studyProtocolQueryCriteria.jsp</result>
		</action>		
		<action name="diseaseConditions" class="gov.nih.nci.pa.action.DiseaseConditionAction" method="getDiseaseConditions">
			<interceptor-ref name="nci-commonsParams" />				         
			<result>/WEB-INF/jsp/protocolDiseaseCondition.jsp</result>
		</action>	
        <action name="studyProtocolView" class="gov.nih.nci.pa.action.StudyProtocolViewAction" method="getStudyProtocol">
            <interceptor-ref name="nci-commonsParams" />                         
            <result>/WEB-INF/jsp/studyProtocolView.jsp</result>
        </action> 
        <action name="studyProtocolEdit" class="gov.nih.nci.pa.action.StudyProtocolViewAction" method="getStudyProtocol">
            <interceptor-ref name="nci-commonsParams" />                         
            <result>/WEB-INF/jsp/studyProtocolEdit.jsp</result>
        </action> 
        
        <action name="studyOverallStatus" class="gov.nih.nci.pa.action.StudyOverallStatusAction">
            <result>/WEB-INF/jsp/studyOverallStatus.jsp</result>
        </action> 
        <action name="studyOverallStatusUpdate" class="gov.nih.nci.pa.action.StudyOverallStatusAction" method="update">
            <result>/WEB-INF/jsp/studyOverallStatus.jsp</result>
        </action> 
        <action name="studyOverallStatusHistory" class="gov.nih.nci.pa.action.StudyOverallStatusAction" method="history">
            <result>/WEB-INF/jsp/studyOverallStatusHistory.jsp</result>
        </action>	
        <action name="participatingOrganizations" class="gov.nih.nci.pa.action.ParticipatingOrganizationsAction">
            <result>/WEB-INF/jsp/participatingOrganizations.jsp</result>
         </action>       
         
        <action name="trialFunding" class="gov.nih.nci.pa.action.TrialFundingAction" method="query">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/WEB-INF/jsp/trialFunding.jsp</result>
			<result name="error">/WEB-INF/jsp/trialFunding.jsp</result>
		</action>	
		<action name="getAuthOrgsAjax" class="gov.nih.nci.pa.action.GetAuthorityOrgsAction" >
			<interceptor-ref name="nci-commonsParams" />				         
			<result>/WEB-INF/jsp/nodecorate/organizations.jsp</result>
		</action>				
		<!-- Wildcards -->
		<action name="trialFunding*" class="gov.nih.nci.pa.action.TrialFundingAction" method="{1}">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/WEB-INF/jsp/trialFundingAdd.jsp</result>
			<result name="error">/WEB-INF/jsp/trialFundingAdd.jsp</result>
		</action>				
		<action name="regulatoryInfo*" class="gov.nih.nci.pa.action.RegulatoryInformationAction" method="{1}">
			<interceptor-ref name="nci-commonsParams" />				         
			<result>/WEB-INF/jsp/RegulatoryInformation.jsp</result>
			<result name="view_page">/WEB-INF/jsp/regulatoryinformationview.jsp</result>
		</action>			
		<action name="nciSpecificInformation*" class="gov.nih.nci.pa.action.NCISpecificInformationAction" method="{1}">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/WEB-INF/jsp/nciSpecificInformation.jsp</result>
			<result name="error">/WEB-INF/jsp/nciSpecificInformation.jsp</result>
		</action>
		<action name="ajaxorg*" class="gov.nih.nci.pa.action.NCISpecificInformationAction" method="{1}">
			<result>/WEB-INF/jsp/nodecorate/{1}.jsp</result>
			<result name="displayOrgFld">/WEB-INF/jsp/nodecorate/orgField.jsp</result>	
		</action>
	</package>
</struts>