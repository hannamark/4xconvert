<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="myPackage" extends="struts-default">
		<interceptors>
		     <interceptor name="nci-commonsParams" class="com.fiveamsolutions.nci.commons.web.struts2.interceptor.DisplayTagParametersInterceptor" />		
		</interceptors>
		<action name="index" class="gov.nih.nci.IndexAction">
			<result>/jsp/index.jsp</result>
		</action>

		<action name="studyProtocolQuery" class="gov.nih.nci.pa.action.StudyProtocolQueryAction" method="query">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/jsp/pajsp/studyProtocolQueryCriteria.jsp</result>
		</action>
		<action name="diseaseConditions" class="gov.nih.nci.pa.action.DiseaseConditionAction" method="getDiseaseConditions">
			<interceptor-ref name="nci-commonsParams" />				         
			<result>/jsp/pajsp/protocolDiseaseCondition.jsp</result>
		</action>	
		<action name="login">
			<result>/jsp/pajsp/login.jsp</result>
		</action>
		<action name="authenticate" class="gov.nih.nci.pa.action.LoginAction">
			<interceptor-ref name="nci-commonsParams" />
			<result name="input">/jsp/pajsp/login.jsp</result>
			<result name="error">/jsp/pajsp/login.jsp</result>
			<result>/jsp/pajsp/studyProtocolQueryCriteria.jsp</result>
		</action>		
		
		<action name="nciSpecificInfo" class="gov.nih.nci.pa.action.NCISpecificInformationAction" method="query">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/jsp/pajsp/nciSpecificInformation.jsp</result>
			<result name="error">/jsp/pajsp/nciSpecificInformation.jsp</result>
		</action>
		
		<action name="updateNCISpecificInfo" class="gov.nih.nci.pa.action.NCISpecificInformationAction" method="update">
		    <interceptor-ref name="nci-commonsParams" />
			<result name="input">/jsp/pajsp/nciSpecificInformation.jsp</result>			
			<result name="error">/jsp/pajsp/nciSpecificInformation.jsp</result>
			<result>/jsp/pajsp/nciSpecificInformationView.jsp</result>
		</action>	
		
				
	</package>
</struts>
