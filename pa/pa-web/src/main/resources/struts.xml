<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="myPackage" extends="struts-default">
		<interceptors>
		     <interceptor name="nci-commonsParams" class="com.fiveamsolutions.nci.commons.web.struts2.interceptor.DisplayTagParametersInterceptor" />		
		</interceptors>
		<action name="index" class="gov.nih.nci.IndexAction">
			<result>/jsp/index.jsp</result>
		</action>
		<action name="helloWorld" class="helloWorldAction"> 
			<result name="input">/jsp/index.jsp</result>
			<result>/jsp/helloWorld.jsp</result>
		</action>		
		<action name="searchProtocol">
			<result>/jsp/pajsp/protocolSearchCriteria.jsp</result>
		</action>
		<action name="queryProtocol" class="gov.nih.nci.pa.action.QueryStudyProtocolAction" method="query">
			<interceptor-ref name="nci-commonsParams" />
			<result>/jsp/pajsp/protocolSearchCriteria.jsp</result>
			<result name="error">/jsp/pajsp/protocolSearchCriteria.jsp</result>
		</action>
		<action name="diseaseConditions" class="gov.nih.nci.pa.action.DiseaseConditionAction" method="getDiseaseConditions">
			<interceptor-ref name="nci-commonsParams" />				         
			<result>/jsp/pajsp/protocolDiseaseCondition.jsp</result>
		</action>	
		<action name="login">
			<result>/jsp/pajsp/login.jsp</result>
		</action>
		<action name="authenticate" class="gov.nih.nci.pa.action.LoginAction">
			<interceptor-ref name="nci-commonsParams" />
			<result name="input">/jsp/pajsp/login.jsp</result>
			<result name="error">/jsp/pajsp/login.jsp</result>
			<result>/jsp/pajsp/protocolSearchCriteria.jsp</result>
		</action>		
		
		<action name="nciSpecificInfo" class="gov.nih.nci.pa.action.NCISpecificInfoAction" method="queryCheck">
			<interceptor-ref name="nci-commonsParams" />			
			<result>/jsp/pajsp/nciSpecificInfo.jsp</result>
			<result name="error">/jsp/pajsp/nciSpecificInfo.jsp</result>
		</action>
		
		<action name="updateNCISpecificInfo" class="gov.nih.nci.pa.action.NCISpecificInfoAction" method="queryUpdate">
		    <interceptor-ref name="nci-commonsParams" />
			<result name="input">/jsp/pajsp/nciSpecificInfo.jsp</result>			
			<result name="error">/jsp/pajsp/nciSpecificInfo.jsp</result>
			<result>/jsp/pajsp/nciSpecificInfo.jsp</result>
		</action>	
		
				
	</package>
</struts>
