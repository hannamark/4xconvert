<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="myPackage" extends="struts-default">
		<action name="index" class="gov.nih.nci.IndexAction">
			<result>/jsp/index.jsp</result>
		</action>
		<action name="helloWorld" class="helloWorldAction"> 
			<result name="input">/jsp/index.jsp</result>
			<result>/jsp/pajsp/login.jsp</result>
		</action>		
		<action name="searchProtocol">
			<result>/jsp/pajsp/protocolSearchCriteria.jsp</result>
		</action>
		<action name="queryProtocol" class="gov.nih.nci.pa.action.ProtocolSearchAction" method="query">
			<result>/jsp/pajsp/protocolSearchCriteria.jsp</result>
		</action>
		<action name="trailDetailEdit">
			<result>/jsp/pajsp/trailDetailEdit.jsp</result>
		</action>		
		<action name="login">
			<result>/jsp/pajsp/login.jsp</result>
		</action>
		<action name="authenticate" class="gov.nih.nci.pa.action.AnnotationAction">
			<result name="input">/jsp/pajsp/login.jsp</result>
			<result name="error">/jsp/pajsp/login.jsp</result>
			<result>/jsp/pajsp/protocolSearchCriteria.jsp</result>
		</action>
	</package>
</struts>
