<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.ctom.domain">
  <class name="StudyInvestigator" table="STUDY_INVESTIGATOR" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      	<generator class="sequence">
		<param name="sequence">STUDY_INVESTIGATOR_SEQ</param>
        </generator>
    </id>
    <property name="responsibilityRoleCode" type="java.lang.String" column="RESPONSIBILITY_ROLE_CODE" />
    <property name="signatureIndicator" type="java.lang.String" column="SIGNATURE_INDICATOR" />
    <property name="signatureText" type="java.lang.String" column="SIGNATURE_TEXT" />
    <property name="startDate" type="java.util.Date" column="START_DATE" />
    <property name="statusCode" type="java.lang.String" column="STATUS_CODE" />
    <property name="stopDate" type="java.util.Date" column="STOP_DATE" />
    <many-to-one name="investigator" class="gov.nih.nci.ctom.domain.Investigator" column="INVESTIGATOR_ID" lazy="proxy" />
    <many-to-one name="protocol" class="gov.nih.nci.ctom.domain.Protocol" column="PROTOCOL_ID" lazy="proxy" />
  </class>
</hibernate-mapping>

