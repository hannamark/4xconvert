<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.ctom.domain">
  <class name="InvestigatorRw" table="INVESTIGATOR" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
        <generator class="sequence">
		<param name="sequence">INVESTIGATOR_SEQ</param>
        </generator>
    </id>
    <property name="administrativeGenderCode" type="java.lang.String" column="ADMINISTRATIVE_GENDER_CODE" />
    <property name="birthDate" type="java.util.Date" column="BIRTH_DATE" />
    <property name="city" type="java.lang.String" column="CITY" />
    <property name="countryCode" type="java.lang.String" column="COUNTRY_CODE" />
    <property name="educationLevelCode" type="java.lang.String" column="EDUCATION_LEVEL_CODE" />
    <property name="employmentStatusCode" type="java.lang.String" column="EMPLOYMENT_STATUS_CODE" />
    <property name="employmentStatusOtherText" type="java.lang.String" column="EMPLOYMENT_STATUS_OTHER_TEXT" />
    <property name="ethnicGroupCode" type="java.lang.String" column="ETHNIC_GROUP_CODE" />
    <property name="firstName" type="java.lang.String" column="FIRST_NAME" />
    <property name="householdIncomeCode" type="java.lang.String" column="HOUSEHOLD_INCOME_CODE" />
    <property name="lastName" type="java.lang.String" column="LAST_NAME" />
    <property name="maritalStatusCode" type="java.lang.String" column="MARITAL_STATUS_CODE" />
    <property name="middleName" type="java.lang.String" column="MIDDLE_NAME" />
    <property name="phone" type="java.lang.String" column="PHONE" />
    <property name="raceCode" type="java.lang.String" column="RACE_CODE" />
    <property name="state" type="java.lang.String" column="STATE" />
    <property name="streetAddress" type="java.lang.String" column="STREET_ADDRESS" />
    <property name="telecomAddress" type="java.lang.String" column="TELECOM_ADDRESS" />
    <property name="zipCode" type="java.lang.String" column="ZIP_CODE" />
    <property name="nciIdentifier" type="java.lang.String" column="NCI_IDENTIFIER" />
  </class>
</hibernate-mapping>

