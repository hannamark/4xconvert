<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.ctom.domain">
  <class name="HealthcareSiteRw" table="HEALTHCARE_SITE" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      	<generator class="sequence">
		<param name="sequence">HEALTHCARE_SITE_SEQ</param>
        </generator>
    </id>
    <property name="city" type="java.lang.String" column="CITY" />
    <property name="countryCode" type="java.lang.String" column="COUNTRY_CODE" />
    <property name="descriptionText" type="java.lang.String" column="DESCRIPTION_TEXT" />
    <property name="name" type="java.lang.String" column="NAME" />
    <property name="postalCode" type="java.lang.String" column="POSTAL_CODE" />
    <property name="stateCode" type="java.lang.String" column="STATE_CODE" />
    <property name="statusCode" type="java.lang.String" column="STATUS_CODE" />
    <property name="statusDate" type="java.util.Date" column="STATUS_DATE" />
    <property name="telecomAddress" type="java.lang.String" column="TELECOM_ADDRESS" />
    <property name="nciInstituteCode" type="java.lang.String" column="NCI_INSTITUTE_CODE" />
  </class>
</hibernate-mapping>

