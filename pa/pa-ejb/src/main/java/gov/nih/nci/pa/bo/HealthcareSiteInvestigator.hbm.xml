<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.ctom.domain">
  <class name="HealthcareSiteInvestigator" table="HEALTHCARE_SITE_INVESTIGATOR" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      	<generator class="sequence">
		<param name="sequence">HEALTHCARE_SITE_INVESTIGTR_SEQ</param>
        </generator>
    </id>
    <property name="roleCode" type="java.lang.String" column="ROLE_CODE" />
    <many-to-one name="investigator" class="gov.nih.nci.ctom.domain.Investigator" column="INVESTIGATOR_ID" lazy="proxy" />
    <many-to-one name="healthcareSite" class="gov.nih.nci.ctom.domain.HealthcareSite" column="HEALTHCARE_SITE_ID" lazy="proxy" />
  </class>
</hibernate-mapping>

