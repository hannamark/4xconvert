<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application PUBLIC
  "-//Sun Microsystems, Inc.//DTD J2EE Application 1.3//EN"
  "http://java.sun.com/dtd/application_1_3.dtd">
<application>
    <display-name>pa-ear</display-name>
    <module>
        <web>
            <web-uri>reg-web.war</web-uri>
            <context-root>/registry</context-root>
        </web>
    </module>
    <module>
        <web>
            <web-uri>pa-web.war</web-uri>
            <context-root>/pa</context-root>
        </web>
    </module>
    @viewer.deployment.descriptor@
    <module>
        <java>jboss-service.xml</java>
    </module>
    <module>
        <ejb>pa-ejb.jar</ejb>
    </module>
    <module>
        <ejb>pa-commons.jar</ejb>
    </module>
	<module>
		<java>commons-collections-3.2.1.jar</java>
	</module>
	<module>
	  <java>commons-io-1.4.jar</java>
	</module>
	<module>
	  <java>commons-fileupload-1.2.1.jar</java>
	</module>
	<module>
	  <java>commons-lang-2.4.jar</java>
	</module>
    <module>
      <java>itext-2.0.6.jar</java>
    </module>
    <security-role><role-name>Abstractor</role-name></security-role>
    <security-role><role-name>Submitter</role-name></security-role>
    <security-role><role-name>ReportViewer</role-name></security-role>
</application>
