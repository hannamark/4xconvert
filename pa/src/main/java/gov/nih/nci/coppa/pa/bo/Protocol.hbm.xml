<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.ctom.domain">
  <class name="Protocol" table="PROTOCOL" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      	<generator class="sequence">
		<param name="sequence">PROTOCOL_SEQ</param>
        </generator>
    </id>
    <property name="amendmentDate" type="java.util.Date" column="AMENDMENT_DATE" />
    <property name="amendmentIdentifier" type="java.lang.Long" column="AMENDMENT_IDENTIFIER" />
    <property name="blindedIndicator" type="java.lang.String" column="BLINDED_INDICATOR" />
    <property name="descriptionText" type="java.lang.String" column="DESCRIPTION_TEXT" />
    <property name="diseaseCode" type="java.lang.String" column="DISEASE_CODE" />
    <property name="documentUri" type="java.lang.String" column="DOCUMENT_URI" />
    <property name="intentCode" type="java.lang.String" column="INTENT_CODE" />
    <property name="longTitleText" type="java.lang.String" column="LONG_TITLE_TEXT" />
    <property name="monitorCode" type="java.lang.String" column="MONITOR_CODE" />
    <property name="multiInstitutionIndicator" type="java.lang.String" column="MULTI_INSTITUTION_INDICATOR" />
    <property name="navyNCIIdentifier" type="java.lang.String" column="NAVY_NCI_IDENTIFIER" />
    <property name="nciIdentifier" type="java.lang.String" column="NCI_IDENTIFIER" />
    <property name="phaseCode" type="java.lang.String" column="PHASE_CODE" />
    <property name="precisText" type="java.lang.String" column="PRECIS_TEXT" />
    <property name="randomizedIndicator" type="java.lang.String" column="RANDOMIZED_INDICATOR" />
    <property name="shortTitleText" type="java.lang.String" column="SHORT_TITLE_TEXT" />
    <property name="sponsorCode" type="java.lang.String" column="SPONSOR_CODE" />
    <property name="sponsorMonitor" type="java.lang.String" column="SPONSOR_MONITOR" />
    <property name="targetAccrualNumber" type="java.lang.Long" column="TARGET_ACCRUAL_NUMBER" />
    <property name="ctomInsertUser" type="java.lang.String" column="CTOM_INSERT_USER" />  
    <set name="studyAgentCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="PROTOCOL_ID" />
      <one-to-many class="gov.nih.nci.ctom.domain.StudyAgent" />
    </set>
    <set name="studySiteCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="PROTOCOL_ID" />
      <one-to-many class="gov.nih.nci.ctom.domain.StudySite" />
    </set>
    <set name="protocolStatusCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="PROTOCOL_ID" />
      <one-to-many class="gov.nih.nci.ctom.domain.ProtocolStatus" />
    </set>
    <set name="studyInvestigatorCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="PROTOCOL_ID" />
      <one-to-many class="gov.nih.nci.ctom.domain.StudyInvestigator" />
    </set>
  </class>
</hibernate-mapping>

