<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.ctom.domain">
  <class name="ProtocolStatus" table="PROTOCOL_STATUS" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
        <generator class="sequence">
		<param name="sequence">PROTOCOL_STATUS_SEQ</param>
        </generator>
    </id>
    <property name="statusCode" type="java.lang.String" column="STATUS_CODE" />
    <property name="statusDate" type="java.util.Date" column="STATUS_DATE" />
    <many-to-one name="protocol" class="gov.nih.nci.ctom.domain.Protocol" column="PROTOCOL_ID" lazy="proxy" />
  </class>
</hibernate-mapping>

