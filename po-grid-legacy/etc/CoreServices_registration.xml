<?xml version="1.0" encoding="UTF-8" ?>
<ServiceGroupRegistrationParameters xmlns:sgc="http://mds.globus.org/servicegroup/client"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/03/addressing"
  xmlns:agg="http://mds.globus.org/aggregator/types"
  xmlns="http://mds.globus.org/servicegroup/client">
  <!-- The ServiceGroupEPR defines the servicegroup to which registrations will be made -->
  <ServiceGroupEPR>
    <wsa:Address>INDEX_SERVICE_URL</wsa:Address>
  </ServiceGroupEPR>
  <!-- Specifies that the registration will be renewed every 10 minutes -->
  <RefreshIntervalSecs>REGISTRATION_REFRESH</RefreshIntervalSecs>
  <Content xsi:type="agg:AggregatorContent" xmlns:agg="http://mds.globus.org/aggregator/types">
    <agg:AggregatorConfig xsi:type="agg:AggregatorConfig">
      <agg:GetMultipleResourcePropertiesPollType
			xmlns:ns12="http://enterpriseservices.nci.nih.gov/structuralroles/HealthCareFacility/types"
			xmlns:ns5="http://enterpriseservices.nci.nih.gov/entities/Person/types"
			xmlns:ns19="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"
			xmlns:ns14="uri:iso.org:21090"
			xmlns:ns0="http://faults.common.coppa.nci.nih.gov"
			xmlns:ns15="http://po.coppa.nci.nih.gov"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:ns3="http://faults.po.coppa.nci.nih.gov"
			xmlns:ns9="http://enterpriseservices.nci.nih.gov/structuralroles/IdentifiedOrganization/types"
			xmlns:ns8="http://enterpriseservices.nci.nih.gov/structuralroles/OrganizationalContact/types"
			xmlns:ns18="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"
			xmlns:ns6="http://enterpriseservices.nci.nih.gov/structuralroles/OversightCommittee/types"
			xmlns:ns10="http://enterpriseservices.nci.nih.gov/structuralroles/IdentifiedPerson/types"
			xmlns:ns16="http://common.coppa.nci.nih.gov"
			xmlns:ns13="http://enterpriseservices.nci.nih.gov/structuralroles/ClinicalResearchStaff/types"
			xmlns:ns1="http://enterpriseservices.nci.nih.gov/structuralroles/ResearchOrganization/types"
			xmlns:ns7="http://enterpriseservices.nci.nih.gov/entities/Organization/types"
			xmlns:ns2="http://schemas.xmlsoap.org/ws/2004/03/addressing"
			xmlns:ns11="http://enterpriseservices.nci.nih.gov/structuralroles/HealthCareProvider/types"
			xmlns:ns17="http://enterpriseservices.nci.nih.gov/CoreServices/types"
			xmlns:ns4="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-BaseFaults-1.2-draft-01.xsd">
        <!-- Specifies that the index should refresh information
            every 300000 milliseconds (once every 5 minutes) -->
        <agg:PollIntervalMillis>INDEX_REFRESH</agg:PollIntervalMillis>
        <!-- specifies all Resource Properties that should be retrieved from the service -->
		<agg:ResourcePropertyNames>ns19:ServiceMetadata</agg:ResourcePropertyNames>
	
      </agg:GetMultipleResourcePropertiesPollType>
    </agg:AggregatorConfig>
    <agg:AggregatorData/>
  </Content>
</ServiceGroupRegistrationParameters>
