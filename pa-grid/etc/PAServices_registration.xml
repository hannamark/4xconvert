<?xml version="1.0" encoding="UTF-8" ?>
<ServiceGroupRegistrationParameters xmlns:sgc="http://mds.globus.org/servicegroup/client"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/03/addressing"
  xmlns:agg="http://mds.globus.org/aggregator/types"
  xmlns="http://mds.globus.org/servicegroup/client">
  <!-- The ServiceGroupEPR defines the servicegroup to which registrations will be made -->
  <ServiceGroupEPR>
    <wsa:Address>INDEX_SERVICE_URL</wsa:Address>
  </ServiceGroupEPR>
  <!-- Specifies that the registration will be renewed every 10 minutes -->
  <RefreshIntervalSecs>REGISTRATION_REFRESH</RefreshIntervalSecs>
  <Content xsi:type="agg:AggregatorContent" xmlns:agg="http://mds.globus.org/aggregator/types">
    <agg:AggregatorConfig xsi:type="agg:AggregatorConfig">
      <agg:GetMultipleResourcePropertiesPollType
			xmlns:ns3="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-BaseFaults-1.2-draft-01.xsd"
			xmlns:ns19="http://enterpriseservices.nci.nih.gov/PAServices/StudyOutcomeMeasureService/types"
			xmlns:ns4="http://schemas.xmlsoap.org/ws/2004/03/addressing"
			xmlns:ns23="http://enterpriseservices.nci.nih.gov/PAServices/StudyContactService/types"
			xmlns:ns9="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.service"
			xmlns:ns1="uri:iso.org:21090"
			xmlns:ns5="http://enterpriseservices.nci.nih.gov/PAServices/ArmService/types"
			xmlns:ns12="http://enterpriseservices.nci.nih.gov/PAServices/StudySiteAccrualStatusService/types"
			xmlns:ns21="http://enterpriseservices.nci.nih.gov/PAServices/StudyIndIdeService/types"
			xmlns:ns22="http://enterpriseservices.nci.nih.gov/PAServices/StudyDiseaseService/types"
			xmlns:ns25="http://enterpriseservices.nci.nih.gov/PAServices/DocumentWorkflowStatusService/types"
			xmlns:ns24="http://enterpriseservices.nci.nih.gov/PAServices/PlannedActivityService/types"
			xmlns:ns0="http://pa.services.coppa.nci.nih.gov"
			xmlns:ns20="http://enterpriseservices.nci.nih.gov/PAServices/StudyOnholdService/types"
			xmlns:xml="http://www.w3.org/XML/1998/namespace"
			xmlns:ns6="http://enterpriseservices.nci.nih.gov/PAServices/types"
			xmlns:ns17="http://enterpriseservices.nci.nih.gov/PAServices/StudyParticipationContactService/types"
			xmlns:ns18="http://enterpriseservices.nci.nih.gov/PAServices/StudyOverallStatusService/types"
			xmlns:ns16="http://enterpriseservices.nci.nih.gov/PAServices/StudyParticipationService/types"
			xmlns:ns7="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"
			xmlns:ns10="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.common"
			xmlns:ns8="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"
			xmlns:ns15="http://enterpriseservices.nci.nih.gov/PAServices/StudyRecruitmentStatusService/types"
			xmlns:ns11="http://enterpriseservices.nci.nih.gov/PAServices/StudyProtocolService/types"
			xmlns:ns13="http://enterpriseservices.nci.nih.gov/PAServices/StudyResourcingService/types"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:ns2="http://faults.pa.services.coppa.nci.nih.gov"
			xmlns:ns26="http://enterpriseservices.nci.nih.gov/PAServices/DocumentService/types"
			xmlns:ns14="http://enterpriseservices.nci.nih.gov/PAServices/StudyRegulatoryAuthorityService/types">
        <!-- Specifies that the index should refresh information
            every 300000 milliseconds (once every 5 minutes) -->
        <agg:PollIntervalMillis>INDEX_REFRESH</agg:PollIntervalMillis>
        <!-- specifies all Resource Properties that should be retrieved from the service -->
		<agg:ResourcePropertyNames>ns8:ServiceMetadata</agg:ResourcePropertyNames>
	
      </agg:GetMultipleResourcePropertiesPollType>
    </agg:AggregatorConfig>
    <agg:AggregatorData/>
  </Content>
</ServiceGroupRegistrationParameters>
