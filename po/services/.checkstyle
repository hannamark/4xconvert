<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <local-check-config name="5AM Checks" location="https://svn.5amsolutions.com/opensource/nci-commons/trunk/core/src/main/resources/5amCheckstyle-5.xml" type="remote" description="">
        <additional-data name="cache-file" value="false"/>
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="5AM Checks" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
        <file-match-pattern match-pattern=".*target.*" include-pattern="false"/>
    </fileset>
    <filter name="FilesFromPackage" enabled="true">
        <filter-data value="target/generated-sources/xsnapshot/java"/>
        <filter-data value="src/test/java"/>
    </filter>
</fileset-config>
