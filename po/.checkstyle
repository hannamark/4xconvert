<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <local-check-config name="5AM Checks" location="https://svn.5amsolutions.com/opensource/nci-commons/trunk/core/src/main/resources/5amCheckstyle.xml" type="remote" description="">
        <additional-data name="cache-file" value="false"/>
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="5AM Checks" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
    <filter name="FilesFromPackage" enabled="true">
        <filter-data value="services/target/generated-sources/xsnapshot/java"/>
        <filter-data value="services/src/test/java"/>
        <filter-data value="web/src/test/java"/>
        <filter-data value="client/src/test/java"/>
    </filter>
</fileset-config>
