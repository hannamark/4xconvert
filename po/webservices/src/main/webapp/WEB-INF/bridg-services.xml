    <bean id="clinicalResearchStaffBridgServiceImpl"
          class="gov.nih.nci.po.webservices.service.bridg.ClinicalResearchStaffServiceImpl" />
    <bean id="clinicalResearchStaffBridgSoapService"
          class="gov.nih.nci.po.webservices.service.bridg.soap.ClinicalResearchStaffPortTypeImpl" />

    <jaxws:endpoint id="clinicalResearchStaffBridgService"
                    implementor="#clinicalResearchStaffBridgSoapService"
                    address="/bridg/ClinicalResearchStaffService"
                    wsdlLocation="/gov/nih/nci/po/webservices/bridg/ClinicalResearchStaff.wsdl"
                    xmlns:bridg="http://enterpriseservices.nci.nih.gov/structuralroles/ClinicalResearchStaff"
                    serviceName="bridg:ClinicalResearchStaffService"
                    endpointName="bridg:ClinicalResearchStaffPortTypePort"
            >

        <jaxws:properties>
            <entry key="schema-validation-enabled" value="false" />
        </jaxws:properties>

    </jaxws:endpoint>


