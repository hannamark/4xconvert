<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <local-check-config name="viewerCheckstyle" location="resources\viewerCheckstyle.xml" type="project" description="">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="src_java" enabled="true" check-config-name="viewerCheckstyle" local="true">
        <file-match-pattern match-pattern="src\\java\\*" include-pattern="true"/>
        <file-match-pattern match-pattern=".svn\\*" include-pattern="false"/>
    </fileset>
</fileset-config>
